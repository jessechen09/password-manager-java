<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.image.ImageView?>

<HBox alignment="CENTER" prefHeight="4.0" prefWidth="598.0" spacing="35.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UserHBoxController">
    <children>
        <Label fx:id="domainLabel" prefWidth="230.0" text="Domain" onMouseClicked="#openWebsite">
            <HBox.margin>
                <Insets />
            </HBox.margin>
        </Label>
        <ImageView fx:id="secureIcon" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true"/>
        <HBox fx:id="accHBox" prefHeight="27.0" prefWidth="257.0" spacing="10.0">
            <children>
                <Button fx:id="copyUsernameButton" alignment="CENTER" mnemonicParsing="false" onAction="#copyUsernameButtonOnAction" onMouseEntered="#buttonOnEnter" onMouseExited="#buttonOnExit" prefWidth="150.0" text="Copy username" textAlignment="CENTER" />
                <Button fx:id="copyPasswordButton" mnemonicParsing="false" onAction="#copyPasswordButtonOnAction" onMouseEntered="#buttonOnEnter" onMouseExited="#buttonOnExit" prefWidth="150.0" text="Copy password" />
                <Button fx:id="deletePasswordButton" mnemonicParsing="false" onMouseEntered="#buttonOnEnter" onMouseExited="#buttonOnExit" prefWidth="150.0" text="Delete" />
            </children>
        </HBox>
    </children>
    <opaqueInsets>
        <Insets bottom="2.0" />
    </opaqueInsets>
    <padding>
        <Insets bottom="5.0" top="5.0" />
    </padding>
</HBox>